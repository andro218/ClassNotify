<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF885B"
    tools:context=".Choices">

    <ImageView
        android:id="@+id/instructorIcon"
        android:layout_width="178dp"
        android:layout_height="187dp"
        android:layout_marginTop="81dp"
        android:layout_marginBottom="110dp"
        android:onClick="showHome"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/studentIcon"
        app:layout_constraintVertical_bias="0.441"
        app:srcCompat="@drawable/instructor_icon" />

    <ImageView
        android:id="@+id/studentIcon"
        android:layout_width="180dp"
        android:layout_height="200dp"
        android:layout_marginTop="110dp"
        android:onClick="showHome2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/student_icon" />
</androidx.constraintlayout.widget.ConstraintLayout>

